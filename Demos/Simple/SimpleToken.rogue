class SimpleToken( type:SimpleTokenType, filepath:String, source:String, line:Int32, column:Int32, content=null:String )
  METHODS
    method cloned->SimpleToken
      return SimpleToken( type, filepath, source, line, column, content )

    method error( message:String )->SimpleError
      return SimpleError( filepath, source, line, column, message )

    method to->String
      if (content) return content
      return type->String
endClass

