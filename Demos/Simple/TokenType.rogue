enum TokenType( symbol:String )
  WS_SPACE(" ")            =  1
  WS_TAB("\x09")           =  2
  WS_NEWLINE("\n")         =  3
  IDENTIFIER("identifier") =  4
  INTEGER("integer")       =  5
  SYMBOL_ASTERISK("*")     =  6
  SYMBOL_CARET("^")        =  7
  SYMBOL_CLOSE_PAREN(")")  =  8
  SYMBOL_EQUALS("=")       =  9
  SYMBOL_MINUS("-")        = 10
  SYMBOL_OPEN_PAREN("(")   = 11
  SYMBOL_PLUS("+")         = 12
  SYMBOL_SLASH("/")        = 13
endEnum
